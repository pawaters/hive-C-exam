/* one loop tthat tries to find the letter of s1 in s2
 * by the end of s2, test if you have gotten to end of s1 */

int	main( int ac, char **av)
{
	while (av[1][i] && av[2][j])
	{
		if (av[1][i] == av[2][j])
		       i++;
		j++;	
	}
	if (av[1][i] = '\n')
		return (1);
	else
		return (0);
	write (1, '\n', 1);
}
